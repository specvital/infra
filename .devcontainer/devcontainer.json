{
  "name": "specvital-infra",
  "dockerComposeFile": "docker-compose.yml",
  "service": "workspace",
  "workspaceFolder": "/workspaces/specvital-infra",
  "features": {
    "ghcr.io/guiyomh/features/just:0": {},
    "./local-features/node": {},
    "./local-features/go": {}
  },
  "postCreateCommand": "bash /workspaces/specvital-infra/.devcontainer/post-create.sh",
  "customizations": {
    "vscode": {
      "extensions": ["esbenp.prettier-vscode", "skellock.just"],
      "settings": {
        "prettier.resolveGlobalModules": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      }
    }
  },
  "remoteUser": "root"
}
